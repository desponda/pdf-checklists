{
  "name": "pdf-checklists",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=@pdf-checklists/frontend & npm run dev --workspace=@pdf-checklists/backend",
    "build": "npm run build --workspace=@pdf-checklists/frontend",
    "test": "npm run test --workspaces",
    "start": "npm run start --workspace=@pdf-checklists/backend",
    "lint": "npm run lint --workspaces",
    "lint:fix": "npm run lint --workspaces -- --fix",
    "format": "npm run format --workspaces",
    "prepare": "husky install"
  },
  "devDependencies": {
    "prettier": "^3.1.0",
    "eslint": "^8.54.0",
    "husky": "^8.0.3"
  }
}
